{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "/static",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "material-ui-dropzone": "^2.3.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf ../backend/static/* ../backend/templates/index.html",
    "build": "npm run clean && react-scripts build && npm run mv-index && npm run mv-static && npm run mv-build && npm run sed && npm run sed1",
    "mv-index": "mv build/index.html ../backend/templates/index.html",
    "mv-static": "mv build/static/* ../backend/static && rm -rf build/static",
    "mv-build": "mv build/* ../backend/static/ && rm -rf build",
    "sed": "sed -i 's+/static/static+/static+g' ../backend/templates/index.html",
    "sed1": "sed -i 's+i.p = \"/static/\"+i.p = \"../\"+g' ../backend/templates/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
